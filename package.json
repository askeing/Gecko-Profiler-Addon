{
  "title": "Gecko Profiler (Hasal)",
  "name": "geckoprofiler-hasal",
  "id": "53aa88a7-38af-4b37-b22f-10c369103ac8@jetpack",
  "version": "2.0.31-hasal20170307",
  "description": "Collect platform profiles from Firefox Desktop.",
  "main": "index.js",
  "author": "Askeing Yen <askeing@gmail.com>",
  "icon": "icon.png",
  "permissions": {
    "multiprocess": true
  },
  "engines": {
    "firefox": ">=38.0a1"
  },
  "license": "MIT",
  "keywords": [
    "jetpack"
  ],
  "preferences": [
    {
      "name": "reportUrl",
      "title": "Reporter URL",
